{
  "name": "physical-ai-robotics-learning-framework",
  "version": "1.0.0",
  "description": "A comprehensive educational platform for Physical AI and Robotics with Docusaurus frontend and FastAPI backend",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"cd backend/app && python -m uvicorn api:app --reload\" \"cd Frontend/Physical_AI && npm start\"",
    "dev:frontend": "cd Frontend/Physical_AI && npm start",
    "dev:backend": "cd backend/app && python -m uvicorn api:app --reload",
    "build:frontend": "cd Frontend/Physical_AI && npm run build",
    "build": "npm run build:frontend",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ai",
    "robotics",
    "education",
    "docusaurus",
    "fastapi",
    "rag",
    "physical-ai"
  ],
